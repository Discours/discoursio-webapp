{
  "routes": [
    {
      "src": "/api/upload",
      "dest": "api/upload.mjs"
    },
    {
      "src": "/api/newsletter",
      "dest": "api/newsletter.js"
    },
    {
      "src": "/api/feedback",
      "dest": "api/feedback.js"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/).*)",
      "destination": "/api/ssr.js"
    }
  ]
}
